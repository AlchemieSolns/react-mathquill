    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.1.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "prettier": "^1.18.2",
    "react-dom": "^16.5.2",
    "string-replace-loader": "^2.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
